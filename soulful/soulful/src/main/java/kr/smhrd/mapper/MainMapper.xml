<mapper namespace="kr.smhrd.mapper.MainMapper">
     <!-- namespace를 다르게 해주면 id값이 같아도 저장된 장소가 달라 상관이 없다. -->
     <!-- 인터페이스 매퍼에 추상메소드이름과 같게 하면 된다. -->
	<!-- VO에 대한 정보가 없으니 넣어줘야 한다!! -->
	
	<!-- 송인포에 먼저 접근해보자 -->
	<select id ="songinfos_List" resultType="kr.smhrd.mapper.SonginfosVO">
	    select * from campus_E_4_1022.songinfos
	</select>
	
	<select id ="songinfos_ListAjax" resultType="kr.smhrd.mapper.SonginfosVO">
	    select * from campus_E_4_1022.songinfos
	</select>
	

	
</mapper>